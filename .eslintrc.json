{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "eol-last": "off",
    "no-multiple-empty-lines": "off",
    "linebreak-style": "off",
    "indent": "off",
    "space-before-blocks": "off",
    "no-underscore-dangle": "off",
    "no-trailing-spaces": "off",
    "no-unused-vars": "off",
    "no-restricted-globals": "off",
    "no-undef": "off"
  }
}
